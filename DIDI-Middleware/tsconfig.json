{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strict": true,
    "module": "commonjs",
    "esModuleInterop": false,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "downlevelIteration": true,
    "paths": {
      "*": ["node_modules/", "src/types/*"]
    }
